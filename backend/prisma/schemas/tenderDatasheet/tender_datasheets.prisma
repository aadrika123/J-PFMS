model tender_datasheets {
  id                  Int               @id @default(autoincrement())
  project_proposal_id Int
  project_proposal    project_proposals @relation(fields: [project_proposal_id], references: [id])
  status              String            @default("draft")
  is_approved         Boolean           @default(false)

  created_at           DateTime                @default(now()) @map("created_at")
  updated_at           DateTime                @default(now()) @map("updated_at")
  tender_basic_detail  tender_basic_details[]
  tender_cover_detail  tender_cover_details[]
  tender_work_detail   tender_work_details[]
  tender_fee_detail    tender_fee_details[]
  tender_critical_date tender_critical_dates[]
  tender_bid_opener    tender_bid_openers[]

  awarded_tender awarded_tenders[]
}