model tender_form_approvals {
   id                        Int               @id @default(autoincrement())
  tender_datasheet_id       Int 
  checker_id                Int
  checker_level             Int?
  assigned_level            Int
  comment                   String
  status                    String

  created_at DateTime @default(now()) @map("created_at")
  updated_at DateTime @default(now()) @map("updated_at")
}